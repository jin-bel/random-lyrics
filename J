<script>
  const lyrics = [
    "https://example.com/lyrics1",
    "https://example.com/lyrics2",
    "https://example.com/lyrics3"
  ];

  const pick = lyrics[Math.floor(Math.random() * lyrics.length)];
  location.replace(pick + "?t=" + Date.now());
</script>
